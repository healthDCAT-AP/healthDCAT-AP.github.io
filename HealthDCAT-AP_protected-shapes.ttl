        sh:path dcat:catalog ;
        sh:severity sh:Violation
    ], [
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:maxCount 1 ;
        sh:path dct:creator ;
        sh:severity sh:Violation
    ], [
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:path dcat:dataset ;
        sh:severity sh:Violation
    ], [
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:description ; 
        sh:severity sh:Violation ;
        sh:message "A dct:description for dcat:Catalogue is required." 
    ], [
